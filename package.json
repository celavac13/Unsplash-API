{
  "name": "unsplash-api",
  "version": "1.0.0",
  "description": "JS practice",
  "main": "script.js",
  "scripts": {
    "compile:sass": "node-sass sass/main.scss css/main.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/main.css -o css/main.prefix.css",
    "compress:css": "node-sass css/main.prefix.css css/main.min.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/celavac13/Unsplash-API.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/celavac13/Unsplash-API/issues"
  },
  "homepage": "https://github.com/celavac13/Unsplash-API#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "node-sass": "^7.0.1",
    "postcss-cli": "^9.1.0"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}